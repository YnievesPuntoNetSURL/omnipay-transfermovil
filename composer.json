{
    "name": "ynievespuntonetsurl/omnipay-transfermovil",
    "type": "library",
    "description": "Transfermovil gateway for Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "Transfermovil",
        "purchase"
    ],
    "homepage": "https://github.com/ynievespuntonetsurl/omnipay-Transfermovil",
    "license": "MIT",
    "authors": [
        {
            "name": "Yoinier Hern√°ndez Nieves",
            "email": "me@ynieves.net"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\Transfermovil\\" : "src/" }
    },
    "require": {
        "php": "^7.2|^8.0",
        "omnipay/common": "^3"
    },
    "require-dev": {
        "omnipay/tests": "^4.1.2",
        "php-http/guzzle7-adapter": "^1.0",
        "symfony/var-dumper": "^6.2"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
